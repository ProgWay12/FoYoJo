<div class="login_form register">
    <div class="page_header">
        <h2>Регистрация</h2>
    </div>
    <form action="">
        <input type="text" name="name" placeholder="ФИО">
        <input type="email" name="email" placeholder="Ваш email">
        <input type="tel" name="tel" placeholder="Ваш телефон">
        <select name="documents" id="">
            <option value="">Документы для въезда в Польшу</option>
            <option value="1">1 документ</option>
            <option value="1">2 документ</option>
            <option value="1">3 документ</option>
            <option value="1">4 документ</option>
        </select>
        <select name="job" id="">
            <option value="">Специальность</option>
            <option value="1">1 Специальность</option>
            <option value="1">2 Специальность</option>
            <option value="1">3 Специальность</option>
            <option value="1">4 Специальность</option>
        </select>
        <select name="language" id="">
            <option value="">Уровень владения языком </option>
            <option value="1">1 Уровень</option>
            <option value="1">2 Уровень</option>
            <option value="1">3 Уровень</option>
            <option value="1">4 Уровень</option>
        </select>
        <textarea name="qualities" id="" cols="30" rows="10" placeholder="Опишите ваши личный качества"></textarea>
        <input type="tel" name="tel" placeholder="Номер whatsapp">
        <input type="tel" name="tel" placeholder="Номер viber">
        <input type="tel" name="tel" placeholder="Номер telegram">
        <input type="password" name="password" placeholder="Пароль">
        <button type="submit">Зарегестрироваться</button>
    </form>
</div>